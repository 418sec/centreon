{
  "name": "Centreon-Hub-UI",
  "version": "1.0.0-beta",
  "description": "GUI for Centreon Hub",
  "main": "build/index.js",
  "scripts": {
    "start": "webpack-dev-server --watch --color --config webpack.dev.js",
    "build:dev": "webpack --config webpack.dev.js",
    "build:preprod": "webpack --config webpack.preprod.js",
    "build:prod": "webpack --config webpack.prod.js",
    "test:run": "jest --watch",
    "test:build": "jest --ci",
    "lint": "standard src/client/**/*.js --fix",
    "test": "npm run lint && npm run test:build",
    "doc:run": "styleguidist server",
    "doc:build": "styleguidist build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/centreon/centreon-hub-ui.git"
  },
  "keywords": [],
  "author": "Centreon Team <centreon@centreon.com>",
  "license": "Apache-2.0",
  "private": true,
  "dependencies": {
    "@devexpress/dx-core": "^1.0.0-rc.2",
    "@devexpress/dx-grid-core": "^1.0.0-rc.2",
    "@devexpress/dx-react-core": "^1.0.0-rc.2",
    "@devexpress/dx-react-grid": "^1.0.0-rc.2",
    "@devexpress/dx-react-grid-material-ui": "^1.0.0-rc.2",
    "axios": "^0.17.1",
    "canvas-prebuilt": "^1.6.5-prerelease.1",
    "chart.js": "^2.7.1",
    "classnames": "^2.2.5",
    "history": "^4.7.2",
    "jss": "^9.6.0",
    "localforage": "^1.5.6",
    "material-ui": "1.0.0-beta.30",
    "material-ui-icons": "^1.0.0-beta.17",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-chartjs-2": "^2.7.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "react-tap-event-plugin": "^3.0.2",
    "redux": "^3.7.2",
    "redux-devtools": "^3.4.1",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.5.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.9",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^4.16.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.6.1",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.4.2",
    "jest": "^22.1.4",
    "jest-css-modules": "^1.1.0",
    "react-docgen": "^2.20.0",
    "react-docgen-docs": "^1.0.3",
    "react-styleguidist": "^6.2.0",
    "react-test-renderer": "^16.2.0",
    "redux-mock-store": "1.4.0",
    "sinon": "^4.2.2",
    "standard": "^10.0.3",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.1"
  },
  "jest": {
    "globals": {
      "env": {
        "apiUrl":"http://127.0.0.1:3000/"
      }
    },
    "moduleNameMapper": {
      "^App[/](.+)": "<rootDir>/src/client/app/$1",
      "^Components[/](.+)": "<rootDir>/src/client/components/$1",
      "^Services[/](.+)": "<rootDir>/src/client/services/$1",
      "^Pages[/](.+)": "<rootDir>/src/client/pages/$1",
      "^Redux[/](.+)": "<rootDir>/src/client/redux/$1"
    },
    "setupFiles": [
      "raf/polyfill"
    ],
    "browser": true,
    "collectCoverage": true
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
