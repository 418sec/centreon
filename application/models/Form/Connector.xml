<?xml version="1.0" encoding="UTF-8"?>
<forms>
	<form name="connector_form">
		<route>/configuration/connector/update</route>
		<redirect>1</redirect>
		<redirect_route>/configuration/connector</redirect_route>
		<section name="General">
			<block name="General information">
				<field name="name" label="Name" default_value="" advanced="0" type="text" parent_field="" module_id="1" mandatory="1"></field>
				<field name="description" label="Description" default_value="" advanced="0" type="text" parent_field="" module_id="1" mandatory="1"></field>
				<field name="command_line" label="Command line" default_value="" advanced="0" type="textarea" parent_field="" module_id="1" mandatory="1"></field>
				<field name="connector_command" label="Commands" default_value="" advanced="0" type="select" parent_field="" module_id="1" mandatory="0">
					<attribute>{"object_type":"object","defaultValuesRoute":"/configuration/command/formlist","listValuesRoute":"/configuration/connector/[i:id]/command","multiple":true,"ordered":false}</attribute>
				</field>
				<field name="enabled" label="Status" default_value="0" advanced="0" type="radio" parent_field="" module_id="1" mandatory="1">
					<attributes>{"Enabled":1, "Disabled":0}</attributes>
				</field>
			</block>
		</section>
	</form>
</forms>
