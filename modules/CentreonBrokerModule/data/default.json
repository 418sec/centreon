{
  "general": {
    "module_directory": "%global_broker_modules_directory%",
    "event_queue_max_size": 10000,
    "flush_logs": "yes",
    "log_thread_id": "no",
    "log_timestamp": "yes",
    "cache_directory": "%global_broker_data_directory%"
  },
  "logger": {
    "config": "yes",
    "debug": "no",
    "error": "yes",
    "info": "yes",
    "level": "low"
  },
  "output": {
    "retry_interval": 60,
    "type": {
      "file": {
        "max_size": 200000000,
        "protocol": "bbdo"
      },
      "rrd": {
        "metrics_path": "%global_rrd_metrics%",
        "status_path": "%global_rrd_status%",
        "path": "%global_rrd_path%",
        "port": "%global_rrd_port%",
        "write_metrics": "yes",
        "write_status": "yes"
      },
      "storage": {
        "db_type": "%global_db_type%",
        "db_host": "%global_db_host%",
        "db_port": "%global_db_port%",
        "db_name": "%global_db_name%",
        "db_user": "%global_db_user%",
        "db_password": "%global_db_password%",
        "queries_per_transaction": 1000,
        "read_timeout": 2,
        "check_replication": 0,
        "store_in_data_bin": "yes",
        "insert_in_index_data": "yes",
        "interval": 1,
        "length": 15552000
      },
      "sql": {
        "buffering_timeout": 10,
        "db_type": "%global_db_type%",
        "db_host": "%global_db_host%",
        "db_port": "%global_db_port%",
        "db_name": "%global_db_name%",
        "db_user": "%global_db_user%",
        "db_password": "%global_db_password%",
        "with_state_events": "yes",
        "queries_per_transaction": 1000,
        "read_timeout": 2,
        "check_replication": 0,
        "interval": "%global_interval%",
        "length": 15552000
      },
      "bam": {
        "db_type": "%global_db_type%",
        "db_host": "%global_db_host%",
        "db_port": "%global_db_port%",
        "db_name": "%global_db_name%",
        "db_user": "%global_db_user%",
        "db_password": "%global_db_password%"
      },
      "bam_bi": {
        "db_type": "%global_db_type%",
        "db_host": "%global_db_host%",
        "db_port": "%global_db_port%",
        "db_name": "%global_db_name%",
        "db_user": "%global_db_user%",
        "db_password": "%global_db_password%"
      },
      "tcp": {
        "one_peer_retention_mode": "no",
        "protocol": "bbdo",
        "negociation": "yes"
      }
    }
  },
  "input": {
    "retry_interval": 60,
    "type": {
      "file": {
        "max_size": 200000000,
        "protocol": "bbdo"
      },
      "rrd": {
        "metrics_path": "%global_rrd_metrics%",
        "status_path": "%global_rrd_status%",
        "path": "%global_rrd_path%",
        "port": "%global_rrd_port%",
        "write_metrics": "yes",
        "write_status": "yes"
      },
      "storage": {
        "db_type": "%global_db_type%",
        "db_host": "%global_db_host%",
        "db_port": "%global_db_port%",
        "db_name": "%global_db_name%",
        "db_user": "%global_db_user%",
        "db_password": "%global_db_password%",
        "queries_per_transaction": 1000,
        "read_timeout": 2,
        "check_replication": 0
      },
      "sql": {
        "buffering_timeout": 10,
        "db_type": "%global_db_type%",
        "db_host": "%global_db_host%",
        "db_port": "%global_db_port%",
        "db_name": "%global_db_name%",
        "db_user": "%global_db_user%",
        "db_password": "%global_db_password%",
        "queries_per_transaction": 1000,
        "with_state_events": "yes",
        "read_timeout": 2,
        "check_replication": 0,
        "interval": "%global_interval%",
        "length": 15552000
      },
      "tcp": {
        "one_peer_retention_mode": "no",
        "protocol": "bbdo",
        "negociation": "yes"
      }
    }
  },
  "statistics": {
    "fifo": "%global_broker_data_directory%/stats.fifo"
  }
}
