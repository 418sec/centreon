{
  "name": "Central",
  "content": {
    "broker": {
      "setup": [
        {
          "name": "Broker Module",
          "params": {
            "mode": [
              {
                "normal": [
                  {
                    "general": {
                      "name": "central-module",
                      "command_file": "%global_broker_data_directory%/central-module.cmd"
                    },
                    "output": [
                      {
                        "type" :"ipv4",
                        "name" : "central-output",
                        "port" : "5668",
                        "host" : "localhost",
                        "protocol": "bbdo",
                        "filters": [
                          {
                            "category": "neb"
                          }
                        ]
                      },
                      {
                        "buffering_timeout": "10",
                        "read_timeout": "10",
                        "retry_interval": "60",
                        "type": "dumper",
                        "tagname": "extcommand-engine-central-module-%poller_id%",
                        "path": "%engine_var_lib_dir%/rw/centengine.cmd",
                        "name": "extcommand-engine-central-module-%poller_name%"
                      },
                      {
                        "buffering_timeout": "10",
                        "read_timeout": "10",
                        "retry_interval": "60",
                        "type": "dumper",
                        "tagname": "extcommand-broker-central-module-%poller_id%",
                        "path": "%global_broker_data_directory%/central-module.cmd",
                        "name": "extcommand-engine-central-module-%poller_name%"
                      },
                      {
                        "buffering_timeout": "10",
                        "read_timeout": "10",
                        "retry_interval": "60",
                        "type": "dumper",
                        "tagname": "cfg-engine-central-module-%poller_id%",
                        "path": "%engine_conf_dir%/$FILENAME$",
                        "name": "cfg-engine-%poller_name%"
                      },
                      {
                        "buffering_timeout": "10",
                        "read_timeout": "10",
                        "retry_interval": "60",
                        "type": "dumper",
                        "tagname": "cfg-broker-central-module-%poller_id%",
                        "path": "%global_broker_etc_directory%/$FILENAME$",
                        "name": "cfg-broker-%poller_name%"
                      }
                    ],
                    "input": [
                      {
                        "type": "tcp",
                        "name": "central-transfert-input",
                        "host": "localhost",
                        "port": "5669"
                      }
                    ],
                    "logger": [
                      {
                        "type": "file",
                        "name": "%global_broker_logs_directory%/central-module.log"
                      }
                    ],
                    "stats": [
                      {
                        "type": "stats",
                        "name": "stats-central-broker",
                        "fifo": "/var/lib/centreon-broker/stats-central-module.fifo"
                      }
                    ]
                  },
                  {
                    "general": {
                      "name": "central-broker",
                      "command_file": "%global_broker_data_directory%/central-broker.cmd",
                      "%callback%": [
                        "pollerCommandLine",
                        "pollerConfigCentreonEngine",
                        "pollerConfigCentreonBroker"
                      ]
                    },
                    "input": [
                      {
                        "type": "tcp",
                        "name": "central-input",
                        "port": "5668"
                      }
                    ],
                    "output": [
                      {
                        "type": "node_events",
                        "name": "notification-ack-downtime",
                        "cache": "yes"
                      },
                      {
                        "type": "sql",
                        "name": "central-sql"
                      },
                      {
                        "type": "storage",
                        "name": "central-storage"
                      },
                      {
                        "type": "ipv4",
                        "name": "central-rrd",
                        "port": "5670",
                        "host": "localhost",
                        "protocol": "bbdo",
                        "tls": "no",
                        "negociation": "yes",
                        "compression": "no"
                      },
                      {
                        "type": "tcp",
                        "name": "central-transfert-output",
                        "port": "5669",
                        "filters": [
                          {
                            "category": "dumper"
                          }
                        ]
                      },
                      {
                        "name": "correlation",
                        "type": "correlation",
                        "file": "%global_broker_etc_directory%/correlation.xml"
                      }
                    ],
                    "logger": [
                      {
                        "type": "file",
                        "name": "%global_broker_logs_directory%/central-broker.log"
                      }
                    ],
                    "stats": [
                      {
                        "type": "stats",
                        "name": "stats-central-broker",
                        "fifo": "/var/lib/centreon-broker/stats-central-broker.fifo"
                      }
                    ]
                  },
                  {
                    "general": {
                      "name": "central-rrd"
                    },
                    "input": [
                      {
                        "type": "ipv4",
                        "name": "central-rrd",
                        "port": "5670",
                        "protocol": "bbdo",
                        "tls": "auto",
                        "negociation": "yes",
                        "compression": "auto"
                      }
                    ],
                    "output": [
                      {
                        "type": "rrd",
                        "name": "storage-rrd"
                      }
                    ],
                    "logger": [
                      {
                        "type": "file",
                        "name": "%global_broker_logs_directory%/central-rrd.log"
                      }
                    ],
                    "stats": [
                      {
                        "type": "stats",
                        "name": "stats-central-rrd",
                        "fifo": "/var/lib/centreon-broker/stats-central-rrd.fifo"
                      }
                    ]
                  }
                ]
              }
            ]
          }
        }
      ]
    }
  }
}
